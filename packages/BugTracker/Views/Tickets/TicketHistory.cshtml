@model IEnumerable<BugTracker.Models.TicketHistory>

@{
    ViewBag.Title = "TicketHistory";
}

<h2>TicketHistory</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
     <tr>
         <th colspan="7">
             Ticket History
         </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Property)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NewValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Changed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TicketId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Property)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NewValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Changed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
      </tr>
}

</table>
<p>
    @Html.ActionLink("Return to Tickets", "TicketsView", "Tickets")
</p>